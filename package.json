    "cron": "cd cron && npm start",
    "cron-dev": "cd cron && npm run dev"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "needle": "^3.2.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.1",
    "url": "^0.11.1",
    "typescript": "^5.1.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/needle": "^3.2.0",
    "@types/node": "^20.4.8"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "tsc-watch": "^6.0.4"
  }
